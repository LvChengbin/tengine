{
    "name": "tengine",
    "version": "0.0.4",
    "description": "Unifying APIs of various of template engines.",
    "main": "lib/index.js",
    "scripts": {
        "test": "jest",
        "test-watch": "jest --watch",
        "test-cov": "jest --coverage --runInBand --forceExit --detectOpenHandles",
        "lint": "eslint lib --cache --ignore-path .eslintignore",
        "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
    },
    "engines": {
        "node": ">=8.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LvChengbin/tengine.git"
    },
    "keywords": [
        "template",
        "consolidate",
        "unify",
        "render",
        "tpl",
        "view",
        "engine",
        "nunjucks",
        "underscore",
        "doT",
        "jade",
        "html",
        "mustache",
        "ejs"
    ],
    "author": "LvChengbin <lvchengbin59@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LvChengbin/tengine/issues"
    },
    "homepage": "https://github.com/LvChengbin/tengine#readme",
    "devDependencies": {
        "eslint": "^5.8.0",
        "jest": "^23.6.0",
        "pre-commit": "^1.2.2",
        "uuid": "^3.3.2"
    },
    "jest": {
        "testMatch": [
            "**/test/**/*.spec.js"
        ],
        "coverageReporters": [
            "text",
            "lcov"
        ],
        "collectCoverageFrom": [
            "lib/**/*.js"
        ],
        "testEnvironment": "node"
    },
    "dependencies": {
        "@lvchengbin/is": "0.0.24",
        "dot": "^1.1.2",
        "dustjs-helpers": "^1.7.4",
        "dustjs-linkedin": "^2.7.5",
        "ejs": "^2.6.1",
        "jade": "^1.11.0",
        "mustache": "^3.0.0",
        "nunjucks": "^3.1.3",
        "underscore": "^1.9.1"
    }
}
